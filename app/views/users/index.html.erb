<div class="list-group">
	<% @users.each do |user| %>
		<div class="list-group-item">
			<%= link_to user.name, user.name %><br>
			<%= user.name %><br>
			<strong>Email:</strong> <%= user.email %><br>
			<strong>Number of pins:</strong> <%= user.pins.count %><br>
			<strong>Current Session Start:</strong> <%= user.current_sign_in_at %><br>
			<strong>Last Session Start:</strong> <%= user.last_sign_in_at %><br>
			<strong>Current Session IP:</strong> <%= user.current_sign_in_ip %><br>
			<strong>Last Session IP:</strong> <%= user.last_sign_in_ip %><br>
			<strong>Number of Sign-ins:</strong> <%= user.sign_in_count %>
	    <% if user == current_user %>
	      <%= link_to edit_user_registration_path do %>
	        <br><span class="glyphicon glyphicon-edit"></span>
	        Edit
	      <% end %>
     	<% end %>
		</div>
	<% end %>
</div>